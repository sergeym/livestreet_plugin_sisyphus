<?xml version="1.0" encoding="UTF-8"?>
<plugin>
        <name>
                <lang name="default">Sisyphus: HTML form content protection</lang>
                <lang name="russian">Sisyphus: защита данных в HTML формах</lang>
        </name>
        <author>
                <lang name="default">Sergey Marin</lang>
                <lang name="russian">Сергей Марин</lang>
        </author>
        <homepage>http://sergeymarin.com/</homepage>
        <version>0.1</version>
        <settings></settings>
        <requires>
                <livestreet>0.4.2</livestreet>
                <plugins>

                </plugins>
        </requires>
        <description>
